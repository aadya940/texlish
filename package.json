{
  "name": "texlish",
  "type": "module",
  "version": "0.0.0",
  "description": "Better LaTeX that compiles to LaTeX",
  "main": "dist/main.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/edemaine/texlish.git"
  },
  "bin": {
    "texlish": "dist/texlish"
  },
  "files": [
    "dist/",
    "CHANGELOG.md",
    "register.js"
  ],
  "engines": {
    "node": ">=19 || ^18.6.0 || ^16.17.0"
  },
  "scripts": {
    "build": "bash ./build/build.sh",
    "docs:dev": "yarn build && vitepress dev civet.dev",
    "docs:build": "yarn build && vitepress build civet.dev",
    "docs:preview": "yarn build && vitepress preview civet.dev",
    "prepublishOnly": "yarn build && yarn test && yarn changelog --release",
    "test": "bash ./build/test.sh",
    "test:self": "yarn build && mocha --config .mocharc-self.json",
    "changelog": "civet build/changelog.civet"
  },
  "author": "Erik Demaine",
  "license": "MIT",
  "devDependencies": {
    "@danielx/civet": "0.7.18",
    "@danielx/hera": "0.8.16",
    "@types/node": "^22.5.5",
    "esbuild": "^0.23.0",
    "typescript": "^5.5.2"
  },
  "packageManager": "pnpm@9.10.0+sha512.73a29afa36a0d092ece5271de5177ecbf8318d454ecd701343131b8ebc0c1a91c487da46ab77c8e596d6acf1461e3594ced4becedf8921b074fbd8653ed7051c"
}
